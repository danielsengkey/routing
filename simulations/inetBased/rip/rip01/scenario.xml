<scenario>
	<at t="60">
        <disconnect src-module="primaryRouter1" src-gate="ethg[0]" />
        <disconnect src-module="primaryRouter2" src-gate="ethg[0]]" />
    </at>
    <at t="90">
        <connect src-module="primaryRouter1" src-gate="ethg[0]"
                 dest-module="primaryRouter2" dest-gate="ethg[0]"
                 channel-type="inet.node.ethernet.Eth1G"/>
    </at>
	<at t="120">
        <disconnect src-module="primaryRouter1" src-gate="ethg[1]" />
        <disconnect src-module="altRouter1" src-gate="ethg[0]]" />
    </at>
	<at t="130">
        <disconnect src-module="primaryRouter1" src-gate="ethg[0]" />
        <disconnect src-module="primaryRouter2" src-gate="ethg[0]]" />
    </at>
     <at t="350">
        <connect src-module="primaryRouter1" src-gate="ethg[1]"
                 dest-module="altRouter1" dest-gate="ethg[0]"
                 channel-type="inet.node.ethernet.Eth100M"/>
    </at>
    <at t="540">
        <connect src-module="primaryRouter1" src-gate="ethg[0]"
                 dest-module="primaryRouter2" dest-gate="ethg[0]"
                 channel-type="inet.node.ethernet.Eth1G"/>
    </at>            
</scenario>